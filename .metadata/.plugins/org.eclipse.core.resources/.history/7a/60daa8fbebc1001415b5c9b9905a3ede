package mx.itesm.a01139626.p3.src;

public class DataSet {
	private double dXK;
	private double dSumX;
	private double dSumY;
	private double dSumXY;
	private double dSumX2;
	private double dSumY2;
	private int iN;
	private double dXAvg;
	private double dYAvg;
	private double dR; 
	private double dR2;
	private double dB0;
	private double dB1;
	private double dYK;
	
	/**
	 * @param dXK
	 * @param dSumX
	 * @param dSumY
	 * @param dSumXY
	 * @param dSumX2
	 * @param dSumY2
	 * @param iN
	 * @param dXAvg
	 * @param dYAvg
	 * @param dR
	 * @param dR2
	 * @param dB0
	 * @param dB1
	 * @param dYK
	 */
	public DataSet(double dXK, double dSumX, double dSumY, double dSumXY,
			double dSumX2, double dSumY2, int iN, double dXAvg, double dYAvg,
			double dR, double dR2, double dB0, double dB1, double dYK) {
		super();
		this.dXK = 0;
		this.dSumX = 0;
		this.dSumY = 0;
		this.dSumXY = 0;
		this.dSumX2 = 0;
		this.dSumY2 = 0;
		this.iN = 0;
		this.dXAvg = 0;
		this.dYAvg = 0;
		this.dR = 0;
		this.dR2 = 0;
		this.dB0 = 0;
		this.dB1 = 0;
		this.dYK = 0;
	}
	
	
	
}
