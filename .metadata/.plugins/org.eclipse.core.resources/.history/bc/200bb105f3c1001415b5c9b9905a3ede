package mx.itesm.a01139626.p3.src;

public class DataSetAnalyzer {
	private String sFileName;
	private DataSet datSet; 
	
	public DataSetAnalyzer() {
		this.sFileName = "";
		this.datSet = null;
	}
	
	public void analyze() {
		
	}
	
	public static void main(String[] args) {
		
		DataSetAnalyzer datAnalyzer = new DataSetAnalyzer();
		IOHandler iohIn = new IOHandler();
		
		datAnalyzer.sFileName = iohIn.readFileName();
		
		datAnalyzer.datSet = iohIn.readDataSetFromFile(datAnalyzer.sFileName, datAnalyzer.datSet);
		
	}

}
